<templateSet group="Perl5 POD">
  <template name="=head1" value="=head1 $TEXT$&#10;&#10;$END$&#10;" description="Header level 1" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="TEXT" expression="&quot;title&quot;" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="=head2" value="=head2 $TEXT$&#10;&#10;$END$&#10;" description="Header level 2" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="TEXT" expression="&quot;title&quot;" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="=head3" value="=head3 $TEXT$&#10;&#10;$END$&#10;" description="Header level 3" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="TEXT" expression="&quot;title&quot;" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="=head4" value="=head4 $TEXT$&#10;&#10;$END$&#10;" description="Header level 4" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="TEXT" expression="&quot;title&quot;" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="=over" value="=over $LEVEL$&#10;&#10;=item $END$&#10;&#10;=back&#10;" description="Items list" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="LEVEL" expression="&quot;1&quot;" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="=begin" value="=begin $FORMAT$ &#10;&#10;$END$&#10;&#10;=end $FORMAT$&#10;" description="Formatted block" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="FORMAT" expression="&quot;html&quot;" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="=for" value="=for $FORMAT$ $END$ &#10;" description="Formatted item" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="FORMAT" expression="&quot;html&quot;" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="=encoding" value="=encoding $ENCODING$ &#10;" description="Encoding" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="ENCODING" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="=item" value="=item $TEXT$ &#10;" description="List item" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="TEXT" expression="&quot;text&quot;" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="=item *" value="=item *&#10;&#10;$END$ &#10;" description="Bulleted list item" toReformat="true" toShortenFQNames="true" deactivated="true" />
</templateSet>